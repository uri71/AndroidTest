            android:name="com.mozidev.firstproject.MainActivity"
            android:label="@string/title_activity_main" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.mozidev.firstproject.TwoActivity"
            android:label="@string/title_activity_two" >
        </activity>
        <activity
            android:name="com.mozidev.firstproject.WebActivity"
            android:label="@string/title_activity_web" >
        </activity>

    </application>

</manifest>
